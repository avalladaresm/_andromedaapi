{
  "name": "api-explorer",
  "version": "0.0.1",
  "description": "api para la plataforma de eCommerce Tecal",
  "main": "index.js",
  "scripts": {
    "lint": "standard --verbose | snazzy",
    "prestart": "npm install",
    "start": "node index.js",
    "dev": "nodemon ./index --exec \"npm run lint && node\"",
    "fix": "standard --fix"
  },
  "eslintConfig": {
    "rules": {
      "no-unused-vars": "on"
    }
  },
  "nodemonConfig": {
    "verbose": true,
    "watch": [
      "views/",
      "app/",
      "api/",
      "config/",
      "controllers/",
      "models/",
      "server.js"
    ]
  },
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "moment": "^2.27.0",
    "nodemon": "^2.0.4",
    "oas3-tools": "^2.0.2",
    "sequelize": "^6.1.0",
    "snazzy": "^8.0.0",
    "tedious": "^8.3.0"
  }
}
